var grEQ = new GlideRecord('proc_po');
grEQ.addEncodedQuery('u_primary_tech=d7262b24dbd88340ae80f8621f96194d^ORu_pm_tech=d7262b24dbd88340ae80f8621f96194d');
grEQ.setLimit(1);
grEQ.setWorkflow(false);
grEQ.queryNoDomain();
while (grEQ.next()) {
		grEQ.u_pm_tech=grEQ.u_primary_tech;
		grEQ.update();
	}  
	